# IntegrityOS Backend Configuration
# Скопируйте этот файл в .env и заполните значения

# Database
# Для SQLite (по умолчанию):
DATABASE_URL=sqlite:///./integrity.db

# Для PostgreSQL (production):
# DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/integrityos

# CORS Origins (список разрешенных доменов)
CORS_ORIGINS=["http://localhost:3000","http://localhost:3001"]

# ML Model Configuration
ML_MODEL_PATH=app/models/ml_model.pkl
ML_MIN_SAMPLES_FOR_TRAINING=100
ML_TEST_SIZE=0.2
ML_RANDOM_STATE=42

# MLflow Configuration
MLFLOW_TRACKING_URI=file:./mlruns
MLFLOW_EXPERIMENT_NAME=integrityos-pipeline

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PREDICTION_CACHE_TTL=3600

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# OpenAI API (опционально, для AI Assistant)
# Получите ключ на https://platform.openai.com/api-keys
OPENAI_API_KEY=

# SQL Query Logging (для отладки)
SQL_ECHO=false
